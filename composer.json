{
    "name": "osit/webalitics",
    "description": "Focused web analytics software, with real-time reports about the traffic sources, locations, and technologies used by the visitors.",
    "version": "1.0.0",
    "keywords": ["Web Analytics","Analytics"],
    "homepage": "https://github.com/OmniSolInfoTech/webalitic",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Osit\\Webalitics\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Osit\\Webalitics\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dominic Moeketsi",
            "email": "developer@osit.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.32",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osit\\Webalitics\\WebaliticsBaseServiceProvider"
            ]
        }
    }
}
