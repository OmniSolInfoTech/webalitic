{
    "name": "osit/webalitics",
    "description": "Focused web analytics software, with real-time reports about the traffic sources, locations, and technologies used by the visitors.",
    "autoload": {
        "psr-4": {
            "Osit\\Webalitics\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Osit\\Webalitics\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dominic",
            "email": "developer@osit.co.za"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.32",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osit\\Webalitics\\WebaliticsBaseServiceProvider"
            ]
        }
    }
}
